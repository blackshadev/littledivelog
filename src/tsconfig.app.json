        "declaration": false,
        "moduleResolution": "node",
        "experimentalDecorators": true,
        "target": "es5",
        "lib": ["es2017", "dom"],
        // "strict": true,
        "outDir": "../out-tsc/app",
        "module": "es2020",
        "baseUrl": "",
